<?xml version="1.0" encoding="utf-8"?>

<resources>

    <declare-styleable name="MultiSwipeRefreshLayout">
        <attr name="foreground" format="reference" />
    </declare-styleable>

    <declare-styleable name="RatioImageView">
        <attr name="imageRatio" format="float" />
        <attr name="imageRatioType">
            <enum name="widthHeight" value="1266" />
            <enum name="heightWidth" value="2266" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RatioTextView">
        <attr name="textRatio" format="float" />
        <attr name="textRatioType">
            <enum name="widthHeight" value="1266" />
            <enum name="heightWidth" value="2266" />
        </attr>
    </declare-styleable>

    <declare-styleable name="VideoRatioTextView">
        <attr name="videoRatio" format="float" />
        <attr name="coverColor" format="color" />
        <attr name="videoRatioType">
            <enum name="widthHeight" value="1266" />
            <enum name="heightWidth" value="2266" />
        </attr>
    </declare-styleable>

</resources>
